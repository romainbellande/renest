{
  "name": "bindex",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:client": "npm start --prefix ./client",
    "start:server": "npm start --prefix ./server",
    "install:all": "yarn && yarn install --cwd ./client && yarn install --cwd ./server",
    "dist:all": "npm run dist --prefix ./server && npm run dist --prefix ./client",
    "dist": "npm run install:all && npm run dist:all",
    "dist:client": "yarn install --cwd ./client && npm run dist --prefix ./client",
    "debug:server": "npm run debug --prefix ./server"
  },
  "dependencies": {
    "@nestjs/swagger": "^1.2.0",
    "concurrently": "^3.5.1",
    "mongoose": "^5.0.1"
  },
  "devDependencies": {
    "@types/mongoose": "^4.7.33",
    "typescript": "^2.6.2"
  }
}
